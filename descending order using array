#include<stdio.h>
#include<conio.h>
void main()
{
int a[i],i,j,temp;
clrscr();
printf("\n Enter array element ");
for(i=0;i<5;i++)
{
scanf"%d",&a[i]);
}
for(i=0;i<5;i++)
{
for(j=i+1;j<5;j++)
{
if(a[i]<a[j])
{
temp=a[i];
a[i]=a[j];
a[j]=temp;
}
}
}
printf("\n The Descending order :\n");
for(i=0;i<5;i++)
{
printf("\n %d",a[i]);
}
getch();
}
